{
	"name": "pens-client",
	"version": "1.0.0",
	"description": "Typescript React test shop project",
	"main": "index.tsx",
	"scripts": {
		"test": "jest --watchAll",
		"start": "npm run prod",
		"build": "npm run prod",
		"dev": "npm run development",
		"prod": "npm run production",
		"base": "cross-env NODE_ENV=none webpack --hide-modules --config ./webpack.base.conf.js --progress",
		"development": "cross-env NODE_ENV=development webpack --hide-modules --config ./webpack.dev.conf.js --watch-poll --watch --progress",
		"production": "cross-env NODE_ENV=production webpack --hide-modules --config ./webpack.prod.conf.js --progress --production"
	},
	"author": "",
	"license": "ISC",
	"keywords": [
		"react",
		"typescript"
	],
	"directories": {
		"test": "tests"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^2.7.0",
		"@typescript-eslint/parser": "^2.7.0",
		"@babel/core": "^7.7.2",
		"@babel/preset-env": "^7.7.1",
		"@babel/preset-react": "^7.7.0",
		"@testing-library/react": "^9.3.2",
		"@types/react": "^16.9.11",
		"@types/react-dom": "^16.9.4",
		"@types/react-router-dom": "^5.1.2",
		"@types/react-transition-group": "^4.2.3",
		"awesome-typescript-loader": "^5.2.1",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"colors": "^1.4.0",
		"eslint": "^6.6.0",
		"css-loader": "^3.2.0",
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-react-hooks": "^2.3.0",
		"cross-env": "^6.0.3",
		"file-loader": "^4.2.0",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^24.9.0",
		"mini-css-extract-plugin": "^0.8.0",
		"node-sass": "^7.0.0",
		"react-test-renderer": "^16.12.0",
		"typescript": "^3.7.2",
		"sass-loader": "^7.3.1",
		"source-map-loader": "^0.2.4",
		"style-loader": "^1.0.0",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"url-loader": "^2.2.0",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.9.0",
		"webpack-merge": "^4.2.2"
	},
	"jest": {
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
			"\\.(css|less|scss|sass)$": "<rootDir>/__mocks__/styleMock.js",
			"^@sass(.*)$": "<rootDir>/src/assets/sass$1",
			"^@components(.*)$": "<rootDir>/src/components$1",
			"^@public(.*)$": "<rootDir>/public$1",
			"^@images(.*)$": "<rootDir>/src/assets/images$1",
			"^@assets(.*)$": "<rootDir>/src/assets$1"
		},
		"moduleFileExtensions": [
			"js",
			"jsx"
		]
	},
	"dependencies": {
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-router-dom": "^5.0.1",
		"react-transition-group": "^4.3.0"
	}
}
