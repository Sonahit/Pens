{
  "name": "pens",
  "version": "1.0.0",
  "description": "React template for internal use",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "start": "npm run prod && npm run server:prod",
    "build": "npm run prod",
    "dev": "npm run development",
    "prod": "npm run production",
    "base": "cross-env NODE_ENV=none webpack --hide-modules --config ./client/webpack.base.conf.js --progress",
    "development": "cross-env NODE_ENV=development webpack --hide-modules --config ./client/webpack.dev.conf.js --watch-poll --watch",
    "production": "cross-env NODE_ENV=production webpack --hide-modules --config ./client/webpack.prod.conf.js --progress --production",
    "server:prod": "cross-env NODE_ENV=production npm run server",
    "server:dev": "cross-env NODE_ENV=development npm run server",
    "server": "cross-env-shell NODE_ENV=$NODE_ENV node ./server/server.js"
  },
  "keywords": [
    "template",
    "nodejs",
    "react"
  ],
  "author": "Ivan Sadykov",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@testing-library/react": "^9.3.1",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "koa-send": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "react-test-renderer": "^16.11.0",
    "sass-loader": "^7.3.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.1.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.7.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/__mocks__/styleMock.js",
      "^@sass(.*)$": "<rootDir>/client/src/assets/sass$1",
      "^@components(.*)$": "<rootDir>/client/src/components$1",
      "^@public(.*)$": "<rootDir>/client/public$1",
      "^@images(.*)$": "<rootDir>/client/src/assets/images$1",
      "^@assets(.*)$": "<rootDir>/client/src/assets$1"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "dependencies": {
    "colors": "^1.4.0",
    "koa": "^2.11.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^4.3.0"
  }
}
