{
  "name": "pens",
  "version": "1.0.0",
  "description": "React template for internal use",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "start": "npm run prod && npm run server:prod",
    "build": "npm run prod",
    "dev": "node build_dev.js",
    "prod": "node build_prod.js",
    "base-win": "SET NODE_ENV=none && npx webpack --config ./client/webpack.base.conf.js --progress",
    "dev-win": "SET NODE_ENV=development && webpack-dev-server --config ./client/webpack.dev.conf.js ",
    "prod-win": "SET NODE_ENV=production && npx webpack --config ./client/webpack.prod.conf.js --progress --production",
    "server:prod-win": "SET NODE_ENV=production && node server/server.js",
    "server:dev-win": "SET NODE_ENV=development && node server/server.js",
    "base-lin": "export NODE_ENV=none && npx webpack --config ./client/webpack.base.conf.js --progress",
    "dev-lin": "export NODE_ENV=development && webpack-dev-server --config ./client/webpack.dev.conf.js ",
    "prod-lin": "export NODE_ENV=production && npx webpack --config ./client/webpack.prod.conf.js --progress --production",
    "server:prod-lin": "export NODE_ENV=production && node server/server.js",
    "server:dev-lin": "export NODE_ENV=development && node server/server.js"
  },
  "keywords": [
    "template",
    "nodejs",
    "react"
  ],
  "author": "Ivan Sadykov",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@testing-library/react": "^9.3.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "react-test-renderer": "^16.11.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/__mocks__/styleMock.js",
      "^@sass(.*)$": "<rootDir>/client/src/assets/sass$1",
      "^@components(.*)$": "<rootDir>/client/src/components$1",
      "^@public(.*)$": "<rootDir>/client/public$1",
      "^@images(.*)$": "<rootDir>/client/src/assets/images$1",
      "^@assets(.*)$": "<rootDir>/client/src/assets$1"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "dependencies": {
    "colors": "^1.4.0",
    "koa": "^2.11.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^4.3.0"
  }
}
